{
  "name": "@contrawork/ui-kit",
  "version": "0.0.1",
  "description": "A React component library implementing Contra's Design System.",
  "repository": "https://github.com/contrawork/ui-kit",
  "author": "Contra Engineering Team <engineering@contra.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "contra",
    "react",
    "ui",
    "uikit"
  ],
  "scripts": {
    "check:package": "yarn package-check",
    "check:tsc": "yarn tsc --noEmit",
    "generate:colors": "node ./scripts/dist/generateColors",
    "generate:textStyles": "node ./scripts/dist/generateTextStyles",
    "prepare": "husky install",
    "test": "yarn jest"
  },
  "devDependencies": {
    "@skypack/package-check": "^0.2.2",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.45",
    "@types/prettier": "^2.2.3",
    "camelcase": "^6.2.0",
    "eslint": "^7.26.0",
    "eslint-config-canonical": "^26.2.3",
    "eslint-config-prettier": "^8.3.0",
    "figma-js": "^1.13.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.21",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  }
}
